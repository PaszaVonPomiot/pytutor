   "source": [
    "name = \"Bob\"\r\n",
    "\r\n",
    "# Old style strings\r\n",
    "\"Hello, %s\" % name\r\n",
    "\r\n",
    "# New style strings\r\n",
    "\"Hello, {}\".format(name)\r\n",
    "\r\n",
    "# f-strings\r\n",
    "f\"Hello, {name}!\"\r\n",
    "\r\n",
    "# Template Strings\r\n",
    "from string import Template\r\n",
    "\r\n",
    "t = Template(\"Hey, $name!\")\r\n",
    "t.substitute(name=name)"
   ],
